{
    "name": "teste",
    "version": 2,
    "public": true,
    "functions": {
      "api/users/**/*.js": {
        "maxDuration": 30
      }
    },
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }